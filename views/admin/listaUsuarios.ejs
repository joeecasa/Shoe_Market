<%- include("../partials/head") %>

    <body>
        <%- include("../partials/header2") %>

            <!-- *****************************INICIO MAIN****************************************** -->

            <main class="main-admin">
                <h3 class="all-product-title" >Usuarios Registrados</h3>
                    <div class="productos-admin">                       
                      
                            <% users.forEach(usuario=> { %> 
                                <!-- <div class="produc">  -->
                                    <section class="section-destacados-admin">
                                                                                  
                                           <img src="../../img/user/<%= usuario.image %>" alt="foto-usuario" >
                                            <p><strong>Nombres y apellidos:</strong> <%= usuario.NombreYapellido %></p>
                                            <p><strong>Correo Electrónico:</strong> <%= usuario.Email %></p>
                                            <p><strong>Fecha de nacimiento: </strong> <%= usuario.FechaNacimiento %></p>
                                            <p><strong>Dirección: </strong> <%= usuario.Domicilio %></p>
                                        
                                            

                                            <form action="usuario/delete/<%= usuario.id %>?_method=DELETE" method="POST" >

                                                <a href="/usuario/editar/<%= usuario.id %>" class="com-but-main"><button class="admin-but">EDITAR</button>
                                                <button type="submt" name="delete" id="delete" class="admin-but">BORRAR</button>
                                            
                        
                                            </form>
                                      
                                    </section> 
                                <!-- </div> -->
                            <% }) %>
                     
                    </div>
                

            </main>

            <!-- ********************************FIN MAIN***************************************** -->
            <%- include("../partials/footer") %>
    </body>

    </html>